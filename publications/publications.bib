@techreport{silvaTemporalSpatialDistribution2022,
  type = {Preprint},
  title = {Temporal and Spatial Distribution Trends of Polio Vaccine Coverage in Children in {{Brazil}}, 2011-2021},
  author = {Silva, T{\'e}rcia Moreira Ribeiro Da and De S{\'a}, Ana Carolina Micheletti Gomide Nogueira and Prates, Elton Junio Sady and Saldanha, Raphael De Freitas and Silva, Thales Philipe Rodrigues Da and Teixeira, Ant{\^o}nia Maria and Beinner, Mark Anthony and De Oliveira, Suelen Rosa and De S{\'a}, Antonio Tolentino Nogueira and Matozinhos, Fernanda Penido and Vieira, Ed Wilson Rodrigues},
  year = {2022},
  month = aug,
  institution = {{In Review}},
  doi = {10.21203/rs.3.rs-1961945/v1},
  urldate = {2023-09-30},
  abstract = {Background: Low polio vaccine coverage can result in the spread of Poliovirus to areas free from viral circulation. This study analyzed the temporal trends and spatial distribution of polio vaccine coverage for children under five years of age in Brazil, between 2011 and 2021. Methods: This is an ecological, time-series study (2011 to 2021) with annual vaccine coverages against poliomyelitis, extracted from the Information System of the National Immunization Program from regions of the 27 Brazilian states. The percentage reductions in vaccination coverage in Brazil and in the Regions were calculated. Prais-Winsten regression models were used to analyze time series for the Regions and States, and spatial analysis identified the distribution of clusters (high-high; low-low; high-low and lowhigh) of vaccination coverages across Brazilian municipalities, using a 5\% significance level. Results: From 2011 to 2021, the coverage of polio vaccines decreased by 46.1\%. There was a progressive increase observed in clusters resulting in low vaccination coverages (136 low-low Brazilian municipalities in 2011 vs 614 in 2021), mostly reported in the North and Northeast regions of the country. There was a downward trend in vaccination coverages in 8 of the 27 States (p {$\leq$} 0.05). Conclusions: The reduction in polio vaccine coverage, as observed in the North and Northeast regions of Brazil, may favor the spread of Poliovirus. Therefore, vaccination strategies should be prioritized for children residing in areas with sharp and recurrent declines in vaccination coverages, including travelers, migrants and refugees.},
  langid = {english},
  file = {/Users/raphaelsaldanha/Zotero/storage/SABB995R/Silva et al. - 2022 - Temporal and spatial distribution trends of polio .pdf}
}

@article{souzajuniorComparisonSamplingDesigns2022b,
  title = {Comparison of Sampling Designs from the Two Editions of the {{{\emph{Brazilian National Health Survey}}}}, 2013 and 2019},
  author = {de Souza J{\'u}nior, Paulo Roberto Borges and Szwarcwald, Celia Landmann and de Almeida, Wanessa da Silva and Damacena, Giseli Nogueira and Pedroso, Marcel de Moraes and de Sousa, Carlos Augusto Moreira and Morais, Igor da Silva and Saldanha, Raphael de Freitas and Lima, Jefferson and Stopa, Sheila Rizzato},
  year = {2022},
