  keywords = {Acesso aos Servi\c{c}os de Sa\'ude,Neoplasias de Mama,Sistemas de Informa\c{c}\~ao},
  file = {/Users/raphaelsaldanha/Zotero/storage/E364A9U8/Saldanha et al. - 2019 - Estudo de análise de rede do fluxo de pacientes de.pdf}
}

