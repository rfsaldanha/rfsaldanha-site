@article{fonsecaGeographicAccessibilityCancer2022a,
  title = {Geographic Accessibility to Cancer Treatment in {{Brazil}}: {{A}} Network Analysis},
  shorttitle = {Geographic Accessibility to Cancer Treatment in {{Brazil}}},
  author = {Fonseca, Bruna de Paula and Albuquerque, Priscila Costa and Saldanha, Raphael de Freitas and Zicker, Fabio},
  year = {2022},
  month = mar,
  journal = {The Lancet Regional Health - Americas},
  volume = {7},
  pages = {100153},
  issn = {2667-193X},
  doi = {10.1016/j.lana.2021.100153},
  urldate = {2023-09-30},
  abstract = {Background Geographic accessibility to healthcare services is a fundamental component in achieving universal health coverage, the central commitment of the Brazilian Unified Health System (SUS). For cancer patients, poor accessibility has been associated with inadequate treatment, worse prognosis, and poorer quality of life. Methods We explored nationwide healthcare data from the SUS health information systems, and mapped the geographic accessibility to cancer treatment in two time-frames: 2009\textendash 2010 and 2017\textendash 2018. We applied social network analysis (SNA) to estimate the commuting route, flow, and distances travelled by cancer patients to undergo surgical, radiotherapy, and chemotherapy treatment. Findings A total of 12,751,728 treatment procedures were analyzed. Overall, more than half of the patients (49{$\cdot$}2 to 60{$\cdot$}7\%) needed to travel beyond their municipality of residence for treatment, a fact that did not change over time. Marked regional differences were observed, as patients living in the northern and midwestern regions of the country had to travel longer distances (weighted average of 296 to 870~km). Cancer care hubs and attraction poles were mostly identified in the southeast and northeast regions, with Barretos being the main hub for all types of treatment throughout time. Interpretation Important regional disparities in the accessibility to cancer treatment in Brazil were revealed, suggesting the need to review the distribution of specialized care in the country. The data presented here contribute to ongoing research on improving access to cancer care and can provide reference to other countries, offering relevant data for oncological and healthcare service evaluation, monitoring, and strategic planning. Funding This work was funded by the Oswaldo Cruz Foundation - Fiocruz (Inova - no. 8451635123 to BPF) and the National Council for Scientific and Technological Development - CNPq (no. 407060/2018\textendash 9 to BPF); Coordination for the Improvement of Higher Education Personnel \textendash{} CAPES (scholarship to PCA, Finance Code 001); and Instituto Nacional de Ci\^encia e Tecnologia de Inova\c{c}\~ao em Doen\c{c}as de Popula\c{c}\~oes Negligenciadas (INCT-IDPN). Resumo A acessibilidade geogr\'afica aos servi\c{c}os de sa\'ude \'e um componente fundamental para o alcance da cobertura universal de sa\'ude, compromisso central do Sistema \'Unico de Sa\'ude (SUS). Para pacientes com c\^ancer, a baixa acessibilidade aos servi\c{c}os especializados tem sido associada ao tratamento inadequado, piora no progn\'ostico e na qualidade de vida. Neste estudo, dados de sa\'ude dos sistemas de informa\c{c}\~ao em sa\'ude do SUS foram utilizados para mapear a acessibilidade geogr\'afica ao tratamento do c\^ancer em dois per\'iodos: 2009\textendash 2010 e 2017\textendash 2018. Aplicamos a an\'alise de redes sociais (ARS) para estimar os fluxos de deslocamento e as dist\^ancias percorridas por pacientes com c\^ancer para receberem tratamento cir\'urgico, radioter\'apico e quimioter\'apico. Um total de 12.751.728 procedimentos de tratamento foram analisados. Em geral, mais da metade dos pacientes (49,2 a 60,7\%) precisaram se deslocar de seus munic\'ipios de resid\^encia para receber tratamento, fato que n\~ao mudou comparando os dois per\'iodos de tempo analisados. Foram observadas importantes diferen\c{c}as regionais no acesso. Pacientes residentes das regi\~oes norte e centro-oeste do pa\'is tiveram que percorrer maiores dist\^ancias para alcan\c{c}ar os servi\c{c}os (m\'edia ponderada~=~296 a 870~km). A maioria dos hubs e polos de atra\c{c}\~ao para atendimento oncol\'ogico foram identificados nas regi\~oes Sudeste e Nordeste, sendo o munic\'ipio de Barretos o principal hub para todos os tipos de tratamento ao longo do tempo. As disparidades de acessibilidade para o tratamento de c\^ancer, alertam para a necessidade de revisar a distribui\c{c}\~ao dos servi\c{c}os de aten\c{c}\~ao especializada no pa\'is. A metodologia e os resultados apresentados neste estudo contribuem para as pesquisas sobre a melhoria do acesso ao tratamento do c\^ancer e podem servir como refer\^encia para outros pa\'ises, oferecendo dados relevantes para avalia\c{c}\~ao, monitoramento e planejamento estrat\'egico de servi\c{c}os oncol\'ogicos e de sa\'ude em geral.},
  keywords = {Brazil,Cancer treatment,Geographic accessibility,Health services access,Social network analysis},
  file = {/Users/raphaelsaldanha/Zotero/storage/GPDPSNH5/Fonseca et al. - 2022 - Geographic accessibility to cancer treatment in Br.pdf;/Users/raphaelsaldanha/Zotero/storage/4ZYSBKTA/S2667193X21001496.html}
}

@article{goncalvesCrescimentoEmpregoIndustrial2019a,
  title = {{Crescimento do emprego industrial local no Brasil: o grau de especializa\c{c}\~ao por intensidade tecnol\'ogica importa?}},
  shorttitle = {{Crescimento do emprego industrial local no Brasil}},
  author = {Gon{\c c}alves, Eduardo and Saldanha, Raphael de Freitas and Almeida, Eduardo and da Silva, Andr{\'e} Suriane},
  year = {2019},
  month = may,
  journal = {Nova Economia},
  volume = {29},
  pages = {41--74},
  publisher = {{Nova Economia}},
  issn = {0103-6351, 1980-5381},
  doi = {10.1590/0103-6351/3301},
  urldate = {2023-09-30},
  abstract = {RESUMO O objetivo desse artigo \'e revisitar o debate sobre o grau de especializa\c{c}\~ao e de diversifica\c{c}\~ao industrial e o crescimento do emprego industrial local no Brasil. Por meio de uma matriz de transbordamentos setoriais, o artigo verifica se setores, agrupados por intensidade tecnol\'ogica, influenciam o desempenho de grupos industriais desagregados. Utilizam-se t\'ecnicas de painel de dados espaciais para controlar efeitos municipais n\~ao observados e poss\'ivel depend\^encia espacial no per\'iodo 1995-2014. Os principais resultados revelam que especializa\c{c}\~oes em grupos setoriais de baixa tecnologia geram transbordamentos para diversos outros grupos industriais, independentemente do n\'ivel de intensidade tecnol\'ogica. Os est\'imulos provenientes de setores de alta tecnologia s\~ao menos frequentes, embora tamb\'em ocorram, dependendo do grupo industrial considerado. Em geral, setores de maior e menor intensidade tecnol\'ogica florescem com a presen\c{c}a de externalidades do tipo MAR. Conclui-se que o debate diversifica\c{c}\~ao/especializa\c{c}\~ao pode variar consideravelmente, o que requer pol\'iticas industriais e regionais espec\'ificas por setores industriais.},
  langid = {portuguese},
  keywords = {Brasil,diversifica\c{c}\~ao,especializa\c{c}\~ao,intensidade tecnol\'ogica industrial,transbordamentos intersetoriais e espaciais},
  file = {/Users/raphaelsaldanha/Zotero/storage/CUHSISZL/Gonçalves et al. - 2019 - Crescimento do emprego industrial local no Brasil.pdf}
}

